"*":
  "atom-material-ui":
    colors:
      abaseColor:
        alpha: 1
        blue: 55
        green: 73
        red: 214
    fonts: {}
  "atom-ternjs": {}
  autosave:
    enabled: true
  core:
    autoHideMenuBar: true
    disabledPackages: [
      "minimap"
    ]
    excludeVcsIgnoredPaths: false
    projectHome: "/home/allan/Workspace"
    themes: [
      "one-dark-ui"
      "monokai"
    ]
  date:
    dateFormat: "YYYYMMDD"
  "disable-keybindings":
    prefixKeys: [
      "ctrl-k"
    ]
  editor:
    invisibles: {}
    scrollPastEnd: true
    showInvisibles: true
  "exception-reporting":
    userId: "863ec07d-08a4-c36d-c936-6ef76f7d343d"
  "file-icons":
    forceShow: true
  linter:
    showErrorTabLine: true
    showErrorTabProject: false
    statusIconScope: "File"
  "markdown-preview-plus": {}
  minimap: {}
  "one-dark-ui":
    layoutMode: "Compact"
  "toggle-packages":
    togglePackages: [
      "autocomplete-plus"
      "linter"
      "vim-mode"
    ]
  "tree-view": {}
  "vim-mode":
    useClipboardAsDefaultRegister: true
  welcome:
    showOnStartup: false
".c.source":
  editor:
    tabLength: 4
".gfm.source":
  editor:
    showInvisibles: false
    softWrap: true
    tabLength: 4
".go.source":
  editor:
    tabLength: 4
".java.source":
  editor:
    tabLength: 4
".python.source":
  editor:
    tabLength: 4
".shell.source":
  editor:
    tabLength: 4
